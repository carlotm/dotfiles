[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = green reverse
    local = green
    remote = yellow
[color "diff"]
    meta = magenta bold
    frag = black bold
    old = red bold
    new = green
[color "status"]
    added = blue
    changed = green
    untracked = yellow
[merge]
    tool = vimdiff
[difftool]
    prompt = false
[http]
    postBuffer = 524288000
[credential]
    helper = cache --timeout=10800
