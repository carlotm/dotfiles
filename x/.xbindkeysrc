# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +1000"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -1000"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Mute microphone
"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
   XF86AudioMicMute

# Screenshoter
"xfce4-screenshooter"
    Print

# Brightness
"brightnessctl s 10%+"
    XF86MonBrightnessUp

"brightnessctl s 10%-"
    XF86MonBrightnessDown
