{
    "elixirLS.dialyzerEnabled": false,
    "coc.preferences.formatOnSaveFiletypes": [
        "elixir",
        "eelixir",
        "python",
        "css",
        "markdown",
        "javascript",
        "javascript.jsx",
        "vue",
        "typescript",
        "typescriptreact"
    ],
    "vetur.ignoreProjectWarning": true,
    "html.filetypes": [
        "html",
        "handlebars",
        "htmldjango",
        "blade",
        "eelixir"
    ],
    "html.autoClosingTags": true,
    "python.formatting.provider": "black",
    "prettier.formatterPriority": 9
}
