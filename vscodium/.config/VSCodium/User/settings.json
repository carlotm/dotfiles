{
    "window.menuBarVisibility": "toggle",
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.tabSize": 2,
    "editor.fontFamily": "Unifont",
    "editor.cursorStyle": "block",
    "editor.wordWrap": "on",
    "editor.folding": false,
    "editor.lineHeight": 30,
    "editor.renderLineHighlight": "all",
    "editor.occurrencesHighlight": false,
    "workbench.editor.tabSizing": "shrink",
    "editor.lineNumbers": "relative",
    "vim.cursorStylePerMode.insert": "underline",
    "vim.highlightedyank.enable": true,
    "vim.visualstar": true,
    "vim.hlsearch": true,
    "terminal.integrated.fontWeightBold": "normal",
    "window.zoomLevel": 1.1,
    "eslint.alwaysShowStatus": true,
    "eslint.autoFixOnSave": true,
    "editor.highlightActiveIndentGuide": false,
    "guides.active.enabled": false,
    "guides.indent.showFirstIndentGuides": false,
    "guides.normal.color.light": "rgba(100, 100, 100, 0.15)",
    "guides.stack.color.light": "rgba(100, 100, 100, 0.15)",
    "guides.active.color.light": "rgba(100, 100, 100, 0.15)",
    "terminal.external.linuxExec": "kitty",
    "terminal.explorerKind": "external",
    "workbench.iconTheme": "material-icon-theme",
    "explorer.confirmDelete": false,
    "workbench.colorTheme": "Nord",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.renderWhitespace": "boundary",
    "workbench.tree.renderIndentGuides": "none",
    "editor.renderIndentGuides": false,
    "editor.fontSize": 13
}
